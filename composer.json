{
  "name": "smalot/cerbere",
  "description": "Drush command line that controls the updates of Drupal modules.",
  "keywords": ["drush", "drupal", "module", "command", "update", "check"],
  "license": "GPL-2.0+",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "authors": [
    { "name": "Sebastien Malot", "email": "sebastien@malot.fr" }
  ],
  "require": {
    "drush/drush": "*",
    "symfony/yaml": "*",
    "swiftmailer/swiftmailer": "*",
    "doctrine/cache": "*",
    "cpliakas/git-wrapper": "~1",
    "monolog/monolog": "~1",
    "symfony/console": "^2.7"
  },
  "require-dev": {
    "atoum/atoum": "*"
  },
  "config": {
    "bin-dir": "bin/"
  },
  "autoload": {
    "psr-0": {
      "Cerbere": "lib/"
    }
  }
}
